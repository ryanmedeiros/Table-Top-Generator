<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ryan.table_top_generator" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SectionSelect"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NPCGenerator"
            android:label="@string/title_activity_npcgenerator"
            android:parentActivityName=".SectionSelect" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.ryan.table_top_generator.SectionSelect" />
        </activity>
    </application>

</manifest>
